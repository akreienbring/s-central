  "description": "Frontend of the S-Central",
  "private": false,
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "vite build --mode private",
    "build release": "vite build --mode release",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
